<% provide(:title, 'All users') %>
<h1>All users</h1>

<%# will_paginate %>

<ul class="users">
   <% @users.each do |user|%>
   <% if current_user != user %>
   <li>
  		<%= avatar(user, size: 52) %>
  		<%= link_to user.full_name, user_path(user) %>
	</li>
<% end %>
<% end %>
</ul>

<%# will_paginate %>